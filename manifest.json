{
    "name": "BasementSports NFTs",
    "version": "0.1.0",
    "description": "This chrome extension is dedicated for BasementSports Extension",
    "permissions": ["storage", "tabs"],
    "host_permissions": [],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "nft.png",
        "24": "nft.png",
        "32": "nft.png"
      },
      "default_title": "BasementSports NFTs",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
  }